msgid "Languages"
msgstr "Taal"

#: On screen: choose_coin
msgid "Rates"
msgstr ""

#: On screen: choose_coin
msgid "Redeem"
msgstr "Inwisselen"

#: On screen: choose-fiat
msgid "How much?"
msgstr "Hoeveel?"

#: On screen: choose-fiat
msgid "Have a promo code?"
msgstr ""

#: On screen: choose-fiat
msgid "Cash Out"
msgstr "Uitbetalen"

#: On screen: choose-fiat
msgid "Clear"
msgstr "Wis"

#: On screen: completed
msgid "Thank you!"
msgstr "Dank u!"

#: On screen: completed
msgid "You Purchased"
msgstr "U Kocht"

#: On screen: completed
msgid "Your coins are on their way to:"
msgstr "Uw coins zijn onderweg naar:"

#: On screen: completed
msgid "Print receipt"
msgstr ""

#: On screen: completed
