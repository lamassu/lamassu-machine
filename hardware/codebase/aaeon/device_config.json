{
  "brain": {
    "freeMemRatio": 0.01,
    "powerStatus": null,
    "dataPath": "/var/lib/lamassu-machine",
    "batteryCapacityPath": null,
    "tempSensorPath": [
      "/sys/class/hwmon/hwmon0/device/temp2_input",
      "/sys/class/hwmon/hwmon1/device/temp2_input"
    ],
    "scanLightOnInterval": null,
    "scanLightOffInterval": 1000,
    "resetBasePath": "/sys/bus/usb/devices"
  },
  "wifi": {
    "wpa": {
      "socket": "/var/run/wpa_supplicant"
    },
    "wpaConfigPath": "/etc/wpa_supplicant.conf"
  },
  "billValidator": {
    "rs232": {
      "device": "/dev/ttyS0"
    }
  },
  "billDispenser": {
    "device": "/dev/ttyS1"
  },
  "scanner": {
    "device": "/dev/video0",
    "qr": {
      "width": 640,
      "height": 480,
      "contrast": 30,
      "lowExposure": 75,
      "highExposure": null,
      "exposureAdjustTime": 200
    },
    "photoId": {
      "width": 1280,
      "height": 960,
      "contrast": 25,
      "lowExposure": null,
      "highExposure": null,
      "exposureAdjustTime": 200
    },
    "photoCard": {
      "width": 1280,
      "height": 960,
      "contrast": 25,
      "lowExposure": null,
      "highExposure": null,
      "exposureAdjustTime": 200,
      "threshold": 6.0,
      "threshold2": 200.0,
      "minFaceSize": 180
    }
  },
  "updater": {
    "caFile": "/var/lib/lamassu-machine/lamassu.pem",
    "downloadDir": "/tmp/download",
    "extractDir": "/tmp/extract",
    "extractor": {
      "lamassuPubKeyFile": "/var/lib/lamassu-machine/lamassu.pub.key"
    }
  }
}
