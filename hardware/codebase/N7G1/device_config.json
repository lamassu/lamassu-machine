{
  "brain": {
    "freeMemRatio": 0.01,
    "dataPath": "/var/lib/sencha",
    "powerStatus": "/sys/class/power_supply/battery/status",
    "batteryCapacityPath": "/sys/class/power_supply/battery/capacity",
    "scanLightOnInterval": null,
    "scanLightOffInterval": 1000,
    "resetBasePath": "/sys/bus/usb/devices/2-1"
  },
  "wifi": {
    "wpa": {
      "socket": "/tmp/wpa_supplicant"
    },
    "wpaConfigPath": "wpa.conf"
  },
  "billValidator": {
    "rs232": {
      "device": "/sys/bus/usb/devices/2-1.3:1.0"
    }
  },
  "billDispenser": {
    "device": "/sys/bus/usb/devices/2-1.5:1.0"
  },
  "scanner": {
    "device": "/dev/video0",
    "qr": {
      "width": 640,
      "height": 480,
      "contrast": 30,
      "lowExposure": 75,
      "highExposure": null,
      "exposureAdjustTime": 200
    },
    "photoId": {
      "width": 1280,
      "height": 960,
      "contrast": 25,
      "lowExposure": null,
      "highExposure": null,
      "exposureAdjustTime": 200
    },
    "photoCard": {
      "width": 1280,
      "height": 960,
      "contrast": 25,
      "lowExposure": null,
      "highExposure": null,
      "exposureAdjustTime": 200,
      "threshold": 6.0,
      "threshold2": 200.0,
      "minFaceSize": 180
    }
  },
  "updater": {
    "caFile": "/usr/local/share/sencha/certs/lamassu.pem",
    "downloadDir": "/tmp/download",
    "extractDir": "/tmp/extract",
    "extractor": {
      "lamassuPubKeyFile": "/usr/local/share/sencha/pubkeys/lamassu.pub.key"
    }
  }
}
